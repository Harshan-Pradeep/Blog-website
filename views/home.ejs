<%- include('./partials/header'); -%>
<div style="margin-top: 100px;">
    <h1><%= title %></h1>
    <p > <%= content %> </p>

    <% posts.forEach((post)=>{ %>
        <h2><%= post.title %></h2>
        <p><%= post.body.substring(0,100)+"..." %><a href="/post/<%= post.title %>">Read More</a></p>
        
    <%})%>

</div>



